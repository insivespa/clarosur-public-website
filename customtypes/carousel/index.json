{
  "id": "carousel",
  "label": "carousel",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": ""
        }
      },
      "project_title": {
        "type": "StructuredText",
        "config": {
          "label": "Project Title",
          "placeholder": "Project name",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "location": {
        "type": "StructuredText",
        "config": {
          "label": "Location",
          "placeholder": "City, State",
          "allowTargetBlank": true,
          "single": "paragraph"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "Describe the project...",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "auto_scroll": {
        "type": "Boolean",
        "config": {
          "label": "Auto Scroll",
          "placeholder_false": "Manual only",
          "placeholder_true": "Auto scroll enabled",
          "default_value": true
        }
      },
      "images": {
        "type": "Group",
        "config": {
          "label": "Images",
          "fields": {
            "image": {
              "type": "Image",
              "config": {
                "label": "Image",
                "constraint": {},
                "thumbnails": []
              }
            }
          }
        }
      }
    }
  }
}